<div class="bs-docs-section">
  <div class="row">
    <div class="col-md-12">


      <div class="page-header">          
        <h1>Listing all Items <br>
          <small><%= pluralize(@contaminationstatuses.count, "Item") %></small></h1>
      </div>

      <div class="col-md-3" id="leftCol">
        <div class="well">    
          <%= render 'layouts/sidebar_show' %>
        </div>
      </div>     

      <div class="col-md-9" id="rightCol"> 
        <%= link_to 'Export XLS', url_for(:format => 'xls'), class: "btn btn-success btn-sm" %> ||
        <%= link_to 'Export CSV', url_for(:format => 'csv'), class: "btn btn-success btn-sm" %> <br><br>
        
        <%= render 'tablecontaminationstatus' %> <br>
        <%= link_to 'New Contaminationstatus', new_contaminationstatus_path, class: "btn btn-success" %>
      </div>
      
    </div>
  </div>
</div>
     
      
