<div class="bs-docs-section">
  <div class="row">
    <div class="col-md-12">


      <div class="page-header">          
        <h1>Listing all Agents <br>
          <small><%= pluralize(@agents.count, "Agent") %></small></h1>
      </div>

      <div class="col-md-3" id="leftCol">
        <div class="well">    
          <%= render 'layouts/sidebar_show' %>
        </div>
      </div>     

      <div class="col-md-9" id="rightCol"> 
        <%= render 'tableagents' %> <br>
        <%= link_to 'New Agent', new_agent_path, class: "btn btn-success " %>
      </div>

    </div>
  </div>
</div>



