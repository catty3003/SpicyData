<div class="bs-docs-section">
  <div class="row">
    <div class="col-md-12">


      <div class="page-header">          
        <h1>Listing all Concentration Results (other than Tenacity)<br>
          <small><%= pluralize(@results.count, "Concentration") %></small></h1>
      </div>

      <div class="col-md-3" id="leftCol">
        <div class="well">    
          <%= render 'layouts/sidebar_show' %>
        </div>
      </div>     

      <div class="col-md-9" id="rightCol">    
        <%= link_to 'New Result', new_result_path, class: "btn btn-success"  %>
        <%= link_to 'Export XLS', url_for(:format => 'xls'), class: "btn btn-success " %> 
        <%= link_to 'Export CSV', url_for(:format => 'csv'), class: "btn btn-success " %> <br><br>                   
        <%= render 'tableresults' %>
        <br>
      </div>
      
    </div>
  </div>
</div>
     
      
