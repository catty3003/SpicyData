<div class="bs-docs-section">
  <div class="row">
    <div class="col-md-12">


      <div class="page-header">          
        <h1>Listing all Spiking Method <br>
          <small><%= pluralize(@spikings.count, "Spiking Method") %></small></h1>
      </div>

      <div class="col-md-3" id="leftCol">
        <div class="well">    
          <%= render 'layouts/sidebar_show' %>
        </div>
      </div>     

      <div class="col-md-9" id="rightCol"> 
        <%= link_to 'New Spiking', new_spiking_path, class: "btn btn-success"  %>   
        <%= link_to 'Export XLS', url_for(:format => 'xls'), class: "btn btn-success " %> 
        <%= link_to 'Export CSV', url_for(:format => 'csv'), class: "btn btn-success " %> <br><br>              
        <%= render 'tablespikings' %>            
        <br>
      </div>

    </div>
  </div>
</div>
     
      

